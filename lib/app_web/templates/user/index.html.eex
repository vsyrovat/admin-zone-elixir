<h1>Users</h1>

<table>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>email / login</th>
    <th>password_hash</th>
  </tr>
  <%= for user <- @users do %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.password_hash %></td>
  </tr>
  <% end %>
</table>
